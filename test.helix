fn function(n) {
    if n <= 1 {
        # io.print("#### n was less than or equal to 1, returning 0")
        return 1       
    } else {
        # io.print(n)
        # let a = function(n-1)
        # let b = function(n-2)

        # io.print("a:", a, "b:", b)

        return function(n-1) + function(n-2)
    }
}

io.print("#############", function(3))
