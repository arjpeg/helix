fn solve (a, b, c) {
    let descriminant = b^2 - 4*a*c

    if (descriminant < 0) {
        return "imaginary"
    }

    else if (descriminant == 0) {
        return -b / (2*a)
    }

    else {
        descriminant = Math.sqrt(descriminant)

        let x1 = (-b + descriminant) / (2*a)
        let x2 = (-b - descriminant) / (2*a)

        return (x1, x2)
    }
}

