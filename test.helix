# Assignment expression
let a = 10

# Arithmetic expression
2 + 3 * 4

# Comparison expression
a == 10 and b <= 20

# Logical expression
(a == 10 or b != 20) and c > 30

# Function definition
func add(x, y) {
    x + y
}

# Function call
add(2, 3)

# If statement
if a > 10 {
    print("a is greater than 10")
} else {
    print("a is less than or equal to 10")
}

# If-else statement with multiple branches
if a > 10 {
    print("a is greater than 10")
} else if a < 10 {
    print("a is less than 10")
} else {
    print("a is equal to 10")
}

# While loop
while a < 10 {
    a = a + 1
}

# For loop
for i in range(0, 10) {
    print(i)
}

# Nested statements
if a > 10 {
    for i in range(0, 10) {
        print(i)
    }
}